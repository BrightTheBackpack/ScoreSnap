{
    "manifest_version": 3,
    "name": "Musescore Jammer",
    "description": "download musescore files",
    "version": "1.0",
    "action": {
      "default_popup": "main.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }

    },
    "content_scripts": [
        {
          "js": ["scripts/attack.js"],
          "matches": [
            "https://musescore.com/*",
            "https://developer.chrome.com/docs/extensions/get-started/tutorial/*"
          ],
          "type": "module"
        }
      ],
    "host_permissions": [
      "https://musescore.com/*"
    ],
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "declarativeContent"
   ]


   
  }